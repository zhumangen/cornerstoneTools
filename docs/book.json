{
  "title": "cornerstone-tools",
  "gitbook": ">3.0.0",
  "plugins": [
    "edit-link",
    "theme-cornerstone",
    "-fontsettings",
    "github",
    "ga",
    "sitemap"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/cornerstonejs/cornerstoneTools/edit/master/docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/cornerstonejs/cornerstoneTools"
    },
    "ga": {
      "token": "UA-110573590-1"
    },
    "sitemap": {
      "hostname": "https://tools.cornerstonejs.org"
    }
  },
  "links": {
    "sharing": {
      "facebook": false,
      "twitter": false
    }
  }
}
